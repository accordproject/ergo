namespace org.accordproject.optfieldtest

define function f(x:Integer?) : Response {
    return Response{
			test1: 0,
			test2: x,
			test3: doubleToInteger(0.0)
		}
}

contract OptFieldTest over TemplateModel {
  // Simple Clause
  clause test(request : Request) : Response {
		return f(some(0 + 0))
  }
}
